class HelloWorld{
      int a = 1;
	  
	  a++;
	  
	  System.out.println(a);
	  
	  
	  
	  int b=10;
	  
	  b--;
	  
	  System.out.println(b);
	  
	  
	  
	  int c=20;
	  
	  int result= ++c;
	  
	  System.out.println(result);
	  
	  int result2=c++;
	  System.out.println(result2);
	  
	  
	  
	  int d=30;
	  
	  int result3= --d;
	  
	  System.out.println(result3);
	  
	  int result4=d--;
	  
	  System.out.println(result4);
	  }
	  
	  
	  
	  